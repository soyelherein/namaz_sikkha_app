{
  "expo": {
    "scheme": "acme",
    "icon" : "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro"
    },
    "name": "Namaz Sikkha",
    "slug": "namaz_sikkha",
    "extra": {
      "eas": {
        "projectId": "4bbbe483-67f0-4eda-8243-8d00a20ee907"
      }
    },
    "ios": {
      "bundleIdentifier": "namaz.sikkha",
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
    },
      "package": "namaz.sikkha"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Namaz Sikkha to use your location."
        }
      ]
    ]
  },
  "icon": ".assets/icon.png"
}
